<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane prefHeight="706.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proj.BoardController">
   <children>
      <StackPane layoutX="-1.0" layoutY="101.0" prefHeight="500.0" prefWidth="600.0">
         <children>
            <GridPane fx:id="gridBoard" alignment="CENTER">
            </GridPane>
         </children>
      </StackPane>
      <Button fx:id="prevMoveBtn" disable="true" layoutX="52.0" layoutY="630.0" mnemonicParsing="false" onAction="#prevMove" opacity="0.0" text="&lt;" />
      <Button fx:id="nextMoveBtn" disable="true" layoutX="520.0" layoutY="630.0" mnemonicParsing="false" onAction="#nextMove" opacity="0.0" text="&gt;" />
      <Button fx:id="playLogBtn" disable="true" layoutX="280.0" layoutY="630.0" mnemonicParsing="false" onAction="#playMode" opacity="0.0" text="Play" />
      <Button fx:id="backToMenuBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToMenu" text="&lt; to menu" />
      <Button fx:id="helpBtn" layoutX="275.0" layoutY="14.0" mnemonicParsing="false" onAction="#onHelpClicked" text="Help" />
      <Label fx:id="timerLabel" layoutX="281.0" layoutY="59.0" />
      <Button fx:id="saveBtn" layoutX="349.0" layoutY="14.0" mnemonicParsing="false" onAction="#saveCUrrentGame" text="Save" />
   </children>
</AnchorPane>
